{
  "name": "<%= scope ? scope+'/' : '' %><%= projectName %>",
  "version": "1.0.0",
  "description": "<%= description ? description : '' %>",
  "main": "plugin/index.js",
  "scripts": {
    "build": "tsc",
    "test": "cd __test__ && cross-env NODE_ENV=test mocha --recursive './**/*spec.js' --exit || exit 1"
  },
  "repository": {
    "type": "git",
    "url": "<%= gitRepo ? 'git+'+gitRepo : '' %>"
  },
  "keywords": [
    "11ty",
    "eleventy"
  ],
  "author": "<%= scope ? scope : '' %>",
  "license": "ISC",
  "bugs": {
    "url": "<%= bugsUrl ? bugsUrl : '' %>"
  },
  "homepage": "<%= homeUrl ? homeUrl : '' %>",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@types/debug": "^4.1.5",
    "@types/node": "^14.14.25",
    "chai": "*",
    "cross-env": "*",
    "mocha": "*",
    "rimraf": "*"
  },
  "dependencies": {
    "debug": "^4.3.1"
  },
  "publishConfig": {
    "access": "<%= publishAccess %>"
  }
}
